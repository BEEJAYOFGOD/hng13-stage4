{
  "expo": {
      "userInterfaceStyle": "light",
      // ... rest of your config

      "name": "framez_tab",
      "slug": "framez_tab",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/images/icon.png",
      "scheme": "frameztab",
      // "userInterfaceStyle": "automatic",
      "newArchEnabled": true,
      "splash": {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
      },
      "ios": {
          "supportsTablet": true
      },
      "android": {
          "adaptiveIcon": {
              "foregroundImage": "./assets/images/adaptive-icon.png",
              "backgroundColor": "#ffffff"
          },
          "edgeToEdgeEnabled": true,
          "predictiveBackGestureEnabled": false
      },
      "web": {
          "bundler": "metro",
          "output": "static",
          "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
          "expo-router",
          [
              "expo-image-picker",
              {
                  "photosPermission": "Allow Framez to access your photos to share posts with your friends.",
                  "cameraPermission": "Allow Framez to use your camera to take photos for posts."
              }
          ]
      ],
      "experiments": {
          "typedRoutes": true
      }
  }
}
